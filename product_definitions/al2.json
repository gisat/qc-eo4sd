{
  "description": "Analytical Layer Level 2",
  "steps": [
    {
      "check_ident": "qc_tool.vector.unzip",
      "required": true
    },
    {
      "check_ident": "eo4sd.vector.naming_eo4sd",
      "required": true,
      "parameters": {
        "layer_regex": "^EO4SD_(?P<place>[a-zA-Z_]+)_AL2$"
      }
    },
    {
      "check_ident": "eo4sd.vector.format",
      "required": true,
      "parameters": {
        "layers":["al"],
        "formats": [".shp"],
        "drivers": {".shp": "ESRI Shapefile"}
      }
    },
    {
      "check_ident": "eo4sd.vector.attribute",
      "required": true,
      "parameters": {
        "layers": ["al"],
        "attributes": {"AL1_ID": ["string", "numeric"],
                       "AL2_ID": ["string", "numeric"],
                       "AL1_NAMEF": ["string"],
                       "AL2_NAMEF": ["string"],
                       "AL2_NAME": ["string"],
                       "AL1_AREA": ["numeric"],
                       "AL2_AREA": ["numeric"]
                       }
      }
    },
    {
      "check_ident": "eo4sd.vector.projection",
      "required": true,
      "parameters": {
        "layers": ["al"],
        "epsg": {
          "DHAKA": 32646,
          "KARACHI": 32642,
          "BAMAKO": 32629,
          "LIMA": 32718,
          "MANDALAY": 32647,
          "PHNOM_PENH": 32648,
          "RAMADI": 32638,
          "FALLUJAH": 32638,
          "RAMADI_FALLUJAH": 32638,
          "VIJAYAWADA": 32644,
          "ARUSHA": 32737,
          "MTWARA": 32737,
          "TANGA": 32737,
          "DODOMA": 32736,
          "MWANZA": 32736,
          "MBEYA": 32736,
          "KIGOMA": 32735,
          "SEMARANG": 32749,
          "DENPASAR": 32750,
          "SAINT_LOUIS": 32628,
          "DAKAR": 32628,
          "ABIDJAN": 32630,
          "BHOPAL": 32643,
          "MELAKA": 32648,
          "BAC_LIEU": 32684,
          "BEN_TRE": 32648,
          "COLOMBO": 32644,
          "DAR_ES_SALAM": 32737,
          "JOHANNESBURG": 32735,
          "KAMPONG_CHHNANG": 32648,
          "KIGALI": 32736,
          "KOLKATA": 32645,
          "LONG_XUYEN": 32648,
          "MAKASSAR": 32750,
          "MENDOZA": 32719,
          "PURSAT": 32648,
          "SEREI_SAOPHOAN": 32648,
          "SOC_TRANG": 32648,
          "STUENG_SAEN": 32648,
          "SUVA": 32760,
          "TAN_AN": 32648,
          "TEGUCIGALPA": 32616,
          "VI_THANH": 32648,
          "VINH_LONG": 32648,
          "CAMPECHE": 32615,
          "MUMBAI": 32643
        }
      }
    },
    {
      "check_ident": "eo4sd.vector.import2pg",
      "required": true
    },
    {
      "check_ident": "qc_tool.vector.unique",
      "required": false,
      "parameters": {
        "layers": ["al"],
        "unique_keys": ["al2_id", "al2_namef", "al2_name"]
      }
    },
    {
      "check_ident": "qc_tool.vector.geometry",
      "required": false,
      "parameters": {
        "layers": ["al"]
      }
    },
    {
      "check_ident": "eo4sd.vector.overlap",
      "required": false,
      "parameters": {
        "layers": ["al"],
        "limit": 1
      }
    }
  ]
}
