{
  "description": "Land Use / Land Cover VHR",
  "steps": [
    {
      "check_ident": "qc_tool.vector.unzip",
      "required": true
    },
    {
      "check_ident": "eo4sd.vector.naming_eo4sd",
      "required": true,
      "parameters": {
        "layer_regex": "^EO4SD_(?P<place>[a-zA-Z_]+)_LULCVHR_(?P<year>[0-9]{4})$"
      }
    },
    {
      "check_ident": "eo4sd.vector.format",
      "required": true,
      "parameters": {
        "layers":["al"],
        "formats": [".shp"],
        "drivers": {".shp": "ESRI Shapefile"}
      }
    },
    {
      "check_ident": "eo4sd.vector.attribute",
      "required": true,
      "parameters": {
        "layers": ["al"],
        "attributes": {"ID": ["string", "numeric"],
                       "C_L1": ["numeric"],
                       "N_L1": ["string"],
                       "C_L2": ["numeric"],
                       "N_L2": ["string"],
                       "C_L3": ["numeric"],
                       "N_L3": ["string"],
                       "C_L4": ["numeric"],
                       "N_L4": ["string"],
                       "AREA": ["numeric"]
                       }
      }
    },
    {
      "check_ident": "eo4sd.vector.projection",
      "required": true,
      "parameters": {
        "layers": ["al"],
        "epsg": {
          "DHAKA": 32646,
          "KARACHI": 32642,
          "BAMAKO": 32629,
          "LIMA": 32718,
          "MANDALAY": 32647,
          "PHNOM_PENH": 32648,
          "RAMADI": 32638,
          "FALLUJAH": 32638,
          "RAMADI_FALLUJAH": 32638,
          "VIJAYAWADA": 32644,
          "ARUSHA": 32737,
          "MTWARA": 32737,
          "TANGA": 32737,
          "DODOMA": 32736,
          "MWANZA": 32736,
          "MBEYA": 32736,
          "KIGOMA": 32735,
          "SEMARANG": 32749,
          "DENPASAR": 32750,
          "SAINT_LOUIS": 32628,
          "DAKAR": 32628,
          "ABIDJAN": 32630,
          "BHOPAL": 32643,
          "MELAKA": 32648,
          "BAC_LIEU": 32684,
          "BEN_TRE": 32648,
          "COLOMBO": 32644,
          "DAR_ES_SALAM": 32737,
          "JOHANNESBURG": 32735,
          "KAMPONG_CHHNANG": 32648,
          "KIGALI": 32736,
          "KOLKATA": 32645,
          "LONG_XUYEN": 32648,
          "MAKASSAR": 32750,
          "MENDOZA": 32719,
          "PURSAT": 32648,
          "SEREI_SAOPHOAN": 32648,
          "SOC_TRANG": 32648,
          "STUENG_SAEN": 32648,
          "SUVA": 32760,
          "TAN_AN": 32648,
          "TEGUCIGALPA": 32616,
          "VI_THANH": 32648,
          "VINH_LONG": 32648,
          "CAMPECHE": 32615,
          "MUMBAI": 32643
        }
      }
    },
    {
      "check_ident": "eo4sd.vector.import2pg",
      "required": true
    },
    {
      "check_ident": "qc_tool.vector.unique",
      "required": false,
      "parameters": {
        "layers": ["al"],
        "unique_keys": ["id"]
      }
    },
    {
      "check_ident": "eo4sd.vector.enum",
      "required": false,
      "parameters": {
        "layers": ["al"],
        "column_defs": [["c_l1", [10000, 20000, 30000, 40000, 50000]],
                        ["c_l2", [11000, 12000, 13000, 14000, 20000, 31000, 32000, 33000, 40000, 51000, 52000]],
                        ["c_l3", [11100, 11200, 12100, 12200, 12300, 12400, 13100, 13300, 13400, 14100, 14200, 14300,
                                  20000,
                                  31000, 32000, 33000,
                                  40000,
                                  51000, 52000]],
                        ["c_l4", [11100,
                                  11210, 11220, 11221, 11222, 11230, 11240,
                                  11300,
                                  12100, 12110, 12111, 12112, 12120, 12121, 12122, 12123, 12124, 12125, 12126, 12127, 12128, 12130,
                                  12210, 12211, 12212, 12220, 12300, 12400,
                                  13100,
                                  13300, 13310, 13400,
                                  14100, 14110,
                                  14200, 14210, 14220, 14230,
                                  14300,
                                  20000,
                                  21000,
                                  31000,
                                  32000,
                                  33000,
                                  40000,
                                  51000,
                                  52000]]]
      }
    },
    {
      "check_ident": "qc_tool.vector.singlepart",
      "required": false,
      "parameters": {
        "layers": ["al"]
      }
    },
    {
      "check_ident": "qc_tool.vector.geometry",
      "required": false,
      "parameters": {
        "layers": ["al"]
      }
    },
    {
      "check_ident": "eo4sd.vector.overlap",
      "required": false,
      "parameters": {
        "layers": ["al"],
        "limit": 1
      }
    }
  ]
}

