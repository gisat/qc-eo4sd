{
  "description": "Urban Green and Open Areas - Extended",
  "steps": [
    {
      "check_ident": "qc_tool.vector.unzip",
      "required": true
    },
    {
      "check_ident": "eo4sd.vector.naming_eo4sd",
      "required": true,
      "parameters": {
        "layer_regex": "^EO4SD_(?P<place>[a-zA-Z_]+)_GREENEXT_(?P<year>[0-9]{4})$"
      }
    },
    {
      "check_ident": "qc_tool.vector.format",
      "required": true,
      "parameters": {
        "layers":["al"],
        "formats": [".shp"],
        "drivers": {".shp": "ESRI Shapefile"}
      }
    },
    {
      "check_ident": "eo4sd.vector.attribute",
      "required": true,
      "parameters": {
        "layers": ["al"],
        "attributes": {"ID": ["string", "numeric"],
                       "CG_L1": ["numeric"],
                       "NG_L1": ["string"],
                       "CG_L2": ["numeric"],
                       "NG_L2": ["string"],
                       "AREA": ["numeric"]
                       }
      }
    },
    {
      "check_ident": "eo4sd.vector.projection",
      "required": true,
      "parameters": {
        "layers": ["al"],
        "epsg": {"DHAKA": 32646,
                 "KARACHI": 32642,
                 "BAMAKO": 32629,
                 "LIMA": 32718,
                 "MANDALAY": 32647,
                 "MWANZA": 32736,
                 "PHNOM_PENH": 32648,
                 "RAMADI_FALLUJAH": 32638
                }
      }
    },
    {
      "check_ident": "eo4sd.vector.import2pg",
      "required": true
    },
    {
      "check_ident": "qc_tool.vector.unique",
      "required": false,
      "parameters": {
        "layers": ["al"],
        "unique_keys": ["id"]
      }
    },
    {
      "check_ident": "qc_tool.vector.enum",
      "required": false,
      "parameters": {
        "layers": ["al"],
        "column_defs": [["cg_l1", [10, 20, 30, 40, 50, 60, 70, 90]],
                        ["cg_l2", [11, 12, 13, 14, 15, 21, 31, 40, 41, 42, 43,
                                   51, 52, 53, 54, 55, 56, 57, 59, 61, 71, 72, 73, 74, 90]]]
      }
    },
    {
      "check_ident": "qc_tool.vector.singlepart",
      "required": false,
      "parameters": {
        "layers": ["al"]
      }
    },
    {
      "check_ident": "qc_tool.vector.geometry",
      "required": false,
      "parameters": {
        "layers": ["al"]
      }
    },
    {
      "check_ident": "qc_tool.vector.overlap",
      "required": false,
      "parameters": {
        "layers": ["al"]
      }
    }
  ]
}
